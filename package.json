{
  "name": "@oxo-ui/worksapce",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "pnpm --filter @oxo-ui/play dev",
    "story": "pnpm --filter @oxo-ui/play storybook",
    "docs:dev": "pnpm --filter @oxo-ui/docs dev",
    "docs:build": "pnpm --filter @oxo-ui/docs build",
    "test": "run-s test-utils test-comp",
    "test-comp": "cross-env NODE_ENV=test pnpm --filter @oxo-ui/components test",
    "test-utils": "cross-env NODE_ENV=test pnpm --filter @oxo-ui/utils test",
    "test-hooks": "cross-env NODE_ENV=test pnpm --filter @oxo-ui/hooks test",
    "build": "run-s build-hooks build-comp",
    "build-comp": "cross-env NODE_ENV=production pnpm --filter oxo-ui build",
    "build-hooks": "cross-env NODE_ENV=production pnpm --filter @oxo-ui/hooks build",
    "build:dev": "cross-env NODE_ENV=development pnpm --filter oxo-ui build:watch",
    "changed": "dotenv lerna version --conventional-commits --create-release github --yes",
    "changed:patch": "dotenv lerna version patch --conventional-commits --create-release github --yes",
    "changed:major": "dotenv lerna version major --conventional-commits --create-release github --yes"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.6",
    "@lerna-lite/cli": "^3.8.0",
    "@lerna-lite/publish": "^3.8.0",
    "@lerna-lite/version": "^3.8.0",
    "@popperjs/core": "^2.11.8",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.20",
    "@types/shelljs": "^0.8.15",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/tsconfig": "^0.5.1",
    "async-validator": "^4.2.5",
    "cross-env": "^7.0.3",
    "npm-run-all": "4.1.5",
    "postcss-color-mix": "^1.1.0",
    "postcss-each": "^1.1.0",
    "postcss-each-variables": "^0.3.0",
    "postcss-for": "^2.1.1",
    "postcss-nested": "^6.0.1",
    "release-it": "^17.3.0",
    "shelljs": "^0.8.5",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^1.4.0",
    "vue-tsc": "^1.8.27",
    "vue3-i18n": "^1.1.5"
  },
  "dependencies": {
    "@oxo-ui/hooks": "workspace:*",
    "@oxo-ui/theme": "workspace:*",
    "@oxo-ui/utils": "workspace:*",
    "@oxo-ui/locale": "workspace:*",
    "@oxo-ui/vite-plugins": "workspace:*",
    "@vitest/coverage-v8": "^1.4.0",
    "@vue/test-utils": "^2.4.5",
    "jsdom": "^24.0.0",
    "lodash-es": "^4.17.21",
    "oxo-ui": "workspace:*",
    "vue": "^3.4.19"
  }
